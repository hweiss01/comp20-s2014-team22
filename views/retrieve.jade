extends layout

block content
	div.container
		div.row
			.col-xs-6
				h1= 'Total Amount Spent: ' + total
			.col-xs-6
				canvas#chart
				script.
					$(document).ready(function(){
					console.log(document.getElementById("web"));
					hello();
					});
		div.panel.panel-default
			div.panel-heading
				table.table.table-striped.table-bordered
					thead
						tr
							th Website
							th Date
							th Total
					tbody
						each item in purchases
							tr
								td#web= item.website
								td#date= item.date
								td#price= item.price
					